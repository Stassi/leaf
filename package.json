{
  "name": "@stassi/leaf",
  "version": "0.0.33",
  "description": "Leaflet adapter.",
  "keywords": [
    "cartography",
    "leaflet",
    "map"
  ],
  "homepage": "https://github.com/Stassi/leaf#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Stassi/leaf.git"
  },
  "license": "MPL-2.0",
  "author": "Andreas Stassivik <andreas@stassi.net>",
  "type": "module",
  "exports": "./dist/leaf.js",
  "types": "./dist/leaf.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rollup -c rollup.config.ts --configPlugin typescript",
    "postbuild": "npm run import:all && npm run lint",
    "import:all": "npm run import:leaf && npm run import:leaflet && npm run import:leaflet:fullscreen",
    "preimport:leaf": "shx mv dist/index.d.ts dist/leaf.d.ts && shx mkdir -p public/leaf",
    "import:leaf": "shx cp -r dist/* public/leaf",
    "preimport:leaflet": "shx mkdir -p public/leaflet",
    "import:leaflet": "shx cp -r node_modules/leaflet/dist/* public/leaflet",
    "preimport:leaflet:fullscreen": "shx mkdir -p public/leaflet/fullscreen",
    "import:leaflet:fullscreen": "shx cp -r node_modules/leaflet-fullscreen/dist/* public/leaflet/fullscreen",
    "lint": "eslint .",
    "prepublishOnly": "npm test",
    "serve": "serve",
    "serve:test": "serve -p 3001",
    "prestart": "npm run build",
    "start": "npm run serve",
    "pretest": "npm run build",
    "test": "jest"
  },
  "prettier": "@vercel/style-guide/prettier",
  "jest": {
    "globalSetup": "jest-environment-puppeteer/setup",
    "globalTeardown": "jest-environment-puppeteer/teardown",
    "maxWorkers": 1,
    "preset": "ts-jest/presets/default-esm",
    "setupFilesAfterEnv": [
      "expect-puppeteer"
    ],
    "testEnvironment": "jest-environment-puppeteer",
    "testTimeout": 10000
  },
  "dependencies": {
    "@types/leaflet": "^1.9.12",
    "leaflet": "^1.9.4",
    "leaflet-fullscreen": "^1.0.2",
    "serve": "^14.2.3"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.25.1",
    "@babel/plugin-syntax-import-attributes": "^7.25.6",
    "@rollup/plugin-commonjs": "^26.0.1",
    "@rollup/plugin-inject": "^5.0.5",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-replace": "^6.0.1",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^11.1.6",
    "@types/jest": "^29.5.13",
    "@types/jest-environment-puppeteer": "^5.0.6",
    "@types/puppeteer": "^5.4.7",
    "@typescript-eslint/eslint-plugin": "^7.1.1",
    "@typescript-eslint/parser": "^7.1.1",
    "@vercel/style-guide": "^6.0.0",
    "dompurify": "^3.1.7",
    "eslint": ">=8.48.0 <9",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.6.3",
    "eslint-plugin-import": "^2.30.0",
    "eslint-plugin-jest": "^28.8.3",
    "eslint-plugin-jsonc": "^2.16.0",
    "eslint-plugin-no-unsanitized": "^4.1.1",
    "eslint-plugin-prettier": "^5.2.1",
    "expect-puppeteer": "^10.1.1",
    "jest": "^29.7.0",
    "jest-environment-puppeteer": "^10.1.1",
    "jest-puppeteer": "^10.1.1",
    "prettier": "3.3.3",
    "puppeteer": "^23.4.0",
    "rollup": "^4.22.4",
    "rollup-plugin-modify": "^3.0.0",
    "shx": "^0.3.4",
    "ts-jest": "^29.2.5",
    "typescript": "^5.6.2"
  },
  "engines": {
    "node": "^18.20.4 || ^20.17.0",
    "npm": "^10.7.0"
  },
  "os": [
    "darwin",
    "linux",
    "win32"
  ],
  "cpu": [
    "arm64",
    "x64"
  ],
  "publishConfig": {
    "access": "public"
  },
  "jest-puppeteer": {
    "launch": {
      "dumpio": true,
      "headless": true
    },
    "server": {
      "command": "npm run serve:test",
      "debug": true,
      "launchTimeout": 10000,
      "port": 3001
    }
  },
  "overrides": {
    "path-to-regexp": "^3.3.0"
  }
}
