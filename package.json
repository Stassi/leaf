{
  "name": "@stassi/leaf",
  "version": "0.0.2",
  "private": true,
  "description": "Leaflet adapter.",
  "keywords": [
    "cartography",
    "leaflet",
    "map"
  ],
  "homepage": "https://github.com/Stassi/leaf#readme",
  "license": "MPL-2.0",
  "author": "Andreas Stassivik <andreas@stassi.net>",
  "type": "module",
  "files": [
    "dist"
  ],
  "exports": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Stassi/leaf.git"
  },
  "scripts": {
    "import:leaflet": "cpx \"node_modules/leaflet/dist/**/*\" public/leaflet",
    "import:leaflet:fullscreen": "cpx \"node_modules/leaflet-fullscreen/dist/**/*\" public/leaflet/fullscreen",
    "prestart": "npm run import:leaflet && npm run import:leaflet:fullscreen",
    "start": "serve public"
  },
  "dependencies": {
    "cpx2": "^7.0.1",
    "leaflet": "^1.9.4",
    "leaflet-fullscreen": "^1.0.2",
    "serve": "^14.2.3"
  },
  "os": [
    "linux",
    "win32"
  ],
  "cpu": [
    "x64"
  ]
}
